{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vjadf2025"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"LS_SQLDB_connect_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLDB_connect'"
		},
		"LS_vjdashboard_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_vjdashboard'"
		},
		"LS_vjdashboard_DL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_vjdashboard_DL'"
		},
		"Ls_AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservervjdashboard.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldbvjdashboard"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloud"
		},
		"LS_SQLDB_connect_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservervjdashboard.database.windows.net"
		},
		"LS_SQLDB_connect_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldbvjdashboard"
		},
		"LS_SQLDB_connect_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloud"
		},
		"LS_vjdashboard_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"LS_vjdashboard_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "world"
		},
		"LS_vjdashboard_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"LS_vjdashboard_DL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vjstorageaccount.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vjadf2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"LS_SQLDB_connect_password": {
						"value": "[parameters('LS_SQLDB_connect_password')]"
					},
					"LS_vjdashboard_DL_accountKey": {
						"value": "[parameters('LS_vjdashboard_DL_accountKey')]"
					},
					"Ls_AzureBlobStorage1_connectionString": {
						"value": "[parameters('Ls_AzureBlobStorage1_connectionString')]"
					},
					"LS_vjdashboard_password": {
						"value": "[parameters('LS_vjdashboard_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"LS_SQLDB_connect_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLDB_connect_properties_typeProperties_server')]"
					},
					"LS_SQLDB_connect_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLDB_connect_properties_typeProperties_database')]"
					},
					"LS_SQLDB_connect_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLDB_connect_properties_typeProperties_userName')]"
					},
					"LS_vjdashboard_DL_properties_typeProperties_url": {
						"value": "[parameters('LS_vjdashboard_DL_properties_typeProperties_url')]"
					},
					"LS_vjdashboard_properties_typeProperties_server": {
						"value": "[parameters('LS_vjdashboard_properties_typeProperties_server')]"
					},
					"LS_vjdashboard_properties_typeProperties_database": {
						"value": "[parameters('LS_vjdashboard_properties_typeProperties_database')]"
					},
					"LS_vjdashboard_properties_typeProperties_username": {
						"value": "[parameters('LS_vjdashboard_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}